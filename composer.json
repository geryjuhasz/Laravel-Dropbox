{
    "name": "graham-campbell/dropbox",
    "description": "Dropbox Is A Dropbox Bridge For Laravel 5",
    "keywords": ["laravel", "framework", "dropbox", "files", "api", "dropbox bridge", "bridge", "Dropbox", "Laravel Dropbox", "Laravel-Dropbox", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*|5.4.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*",
        "graham-campbell/manager": "^2.3",
        "alorel/dropbox-v2-php": "^0.4.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Dropbox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Dropbox\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
